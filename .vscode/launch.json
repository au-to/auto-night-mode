{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome with extension",
      "url": "http://localhost",
      "webRoot": "${workspaceFolder}/dist",
      "runtimeArgs": [
        "--disable-extensions-except=${workspaceFolder}",
        "--load-extension=${workspaceFolder}"
      ]
    }
  ]
}
